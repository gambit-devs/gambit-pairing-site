    <script>
        // --- NEW: Improved Hamburger menu toggle for mobile nav with focus trap and ESC close ---
        // --- VERSION VARIABLE ---
        const VERSION = "0.4.1";
        function toggleNav(forceClose = false) {
            var navLinks = document.getElementById('navbar-links');
            var toggleBtn = document.getElementById('navbar-toggle');
            var overlay = document.getElementById('navbar-overlay');
            var isOpen = navLinks.classList.contains('open');
            if (forceClose || isOpen) {
                navLinks.classList.remove('open');
                toggleBtn.classList.remove('is-active');
                document.body.style.overflow = '';
                // Hide overlay
                if (overlay) overlay.classList.remove('open');
                // Remove focus trap
                document.removeEventListener('keydown', navFocusTrap);
            } else {
                navLinks.classList.add('open');
                toggleBtn.classList.add('is-active');
                document.body.style.overflow = 'hidden';
                // Show overlay
                if (overlay) overlay.classList.add('open');
                // Focus first link
                setTimeout(function () {
                    var firstLink = navLinks.querySelector('a');
                    if (firstLink) firstLink.focus();
                }, 100);
                // Add focus trap
                document.addEventListener('keydown', navFocusTrap);
            }
        }
        // Focus trap for mobile nav
        function navFocusTrap(e) {
            var navLinks = document.getElementById('navbar-links');
            if (!navLinks.classList.contains('open')) return;
            var focusable = navLinks.querySelectorAll('a');
            if (!focusable.length) return;
            var first = focusable[0], last = focusable[focusable.length - 1];
            if (e.key === 'Tab') {
                if (e.shiftKey && document.activeElement === first) {
                    last.focus();
                    e.preventDefault();
                } else if (!e.shiftKey && document.activeElement === last) {
                    first.focus();
                    e.preventDefault();
                }
            }
            if (e.key === 'Escape') {
                toggleNav(true);
                document.getElementById('navbar-toggle').focus();
            }
        }
        document.addEventListener('DOMContentLoaded', function () {
            var navLinks = document.getElementById('navbar-links');
            var links = navLinks.querySelectorAll('a');
            var overlay = document.getElementById('navbar-overlay');
            // --- VERSION PATCH: Update versioned text and links ---
            // Update alert version text
            var alertEl = document.querySelector('.alert');
            if (alertEl) {
                alertEl.innerHTML = `Alpha <span id="app-version">${VERSION}</span> &mdash; Features and stability are not guaranteed.`;
            }
            // Update MSI download link
            var msiBtn = document.querySelector('a.download[href*="Gambit.Pairing-"]');
            if (msiBtn) {
                msiBtn.href = `https://github.com/Chickaboo/Gambit-Pairing/releases/latest/download/Gambit.Pairing-${VERSION}-win64.msi`;
            }
            // Close menu on link click (mobile)
            links.forEach(function (link) {
                link.addEventListener('click', function () {
                    toggleNav(true);
                });
            });
            // Remove old click-outside logic
            // navLinks.addEventListener('click', function(e) {
            //     if (e.target === navLinks) toggleNav(true);
            // });
            // New: close menu if clicking overlay
            if (overlay) {
                overlay.addEventListener('click', function () {
                    toggleNav(true);
                });
            }
            // Gallery modal logic
            const screenshots = [
                {
                    src: "../docs/screenshots/tournament.png",
                    caption: "Tournament Control Tab"
                },
                {
                    src: "../docs/screenshots/player.png",
                    caption: "Player Manager Tab"
                },
                {
                    src: "../docs/screenshots/standings.png",
                    caption: "Standings Tab"
                },
                {
                    src: "../docs/screenshots/crosstable.png",
                    caption: "Cross-Table Tab"
                },
                {
                    src: "../docs/screenshots/history_log.png",
                    caption: "History Log Tab"
                },
                {
                    src: "../docs/screenshots/print_dialog.png",
                    caption: "Print Dialog"
                }
            ];
            const gridImgs = document.querySelectorAll('.screenshot-img');
            const modal = document.getElementById('gallery-modal');
            const modalImg = document.getElementById('gallery-modal-img');
            const modalCaption = document.getElementById('gallery-modal-caption');
            const closeBtn = document.getElementById('gallery-modal-close');
            const leftBtn = document.getElementById('gallery-modal-arrow-left');
            const rightBtn = document.getElementById('gallery-modal-arrow-right');
            let currentIdx = 0;

            function openModal(idx) {
                currentIdx = idx;
                modalImg.src = screenshots[idx].src;
                modalImg.alt = screenshots[idx].caption;
                modalCaption.textContent = screenshots[idx].caption;
                modal.classList.add('open');
                document.body.style.overflow = 'hidden';
            }
            function closeModal() {
                modal.classList.remove('open');
                document.body.style.overflow = '';
            }
            function showPrev() {
                currentIdx = (currentIdx + screenshots.length - 1) % screenshots.length;
                openModal(currentIdx);
            }
            function showNext() {
                currentIdx = (currentIdx + 1) % screenshots.length;
                openModal(currentIdx);
            }
            gridImgs.forEach((img, idx) => {
                img.addEventListener('click', () => openModal(idx));
                img.addEventListener('keydown', (e) => {
                    if (e.key === 'Enter' || e.key === ' ') openModal(idx);
                });
            });
            closeBtn.addEventListener('click', closeModal);
            leftBtn.addEventListener('click', showPrev);
            rightBtn.addEventListener('click', showNext);
            modal.addEventListener('click', function (e) {
                if (e.target === modal) closeModal();
            });
            document.addEventListener('keydown', function (e) {
                if (!modal.classList.contains('open')) return;
                if (e.key === 'Escape') closeModal();
                if (e.key === 'ArrowLeft') showPrev();
                if (e.key === 'ArrowRight') showNext();
            });

            // FAQ accordion logic
            document.querySelectorAll('.faq-q').forEach(function (q) {
                q.addEventListener('click', function () {
                    const item = q.closest('.faq-item');
                    if (item.classList.contains('open')) {
                        item.classList.remove('open');
                    } else {
                        document.querySelectorAll('.faq-item.open').forEach(i => i.classList.remove('open'));
                        item.classList.add('open');
                    }
                });
                q.addEventListener('keydown', function (e) {
                    if (e.key === 'Enter' || e.key === ' ') q.click();
                });
            });
        });
    </script>
</head>

