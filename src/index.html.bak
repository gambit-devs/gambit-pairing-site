    <style>
        :root {
            --black: #181a1b;
            --white: #fff;
            --gray: #e5e5e5;
            --muted: #888;
            --accent: #e2c290;
            --accent-dark: #bfa36e;
            --green: #3b5e2b;
            --green-dark: #2d471f;
            --beige: #f5e9da;
            --shadow: 0 6px 32px 0 rgba(0, 0, 0, 0.10);
            --radius-lg: 28px;
            --radius-md: 18px;
            --radius-sm: 8px;
            --transition: 0.18s cubic-bezier(.4, 0, .2, 1);
        }

        html,
        body {
            background: linear-gradient(120deg, #f8f4ee 0%, #e2c290 100%);
            color: var(--black);
            font-family: 'Inter', 'Segoe UI', Arial, sans-serif;
            margin: 0;
            padding: 0;
            font-size: 17px;
            line-height: 1.7;
            scroll-behavior: smooth;
        }

        body {
            min-height: 100vh;
            display: flex;
            flex-direction: column;
            position: relative;
            overflow-x: hidden;
        }

        /* --- BACKGROUND TEXT --- */
        .bg-text-gambit {
            position: fixed;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            font-family: 'Inter', 'Segoe UI', Arial, sans-serif;
            font-weight: 900;
            font-size: 8vw;
            color: #181a1b;
            opacity: 0.06;
            letter-spacing: 0.04em;
            white-space: nowrap;
            z-index: 0;
            pointer-events: none;
            user-select: none;
            text-shadow: 0 2px 24px rgba(0, 0, 0, 0.09), 0 0 80px #e2c290;
            filter: blur(0.5px);
            transition: opacity 0.4s;
            word-spacing: 650px;
        }

        @media (max-width: 900px) {
            .bg-text-gambit {
                font-size: 13vw;
            }
        }

        @media (max-width: 600px) {
            .bg-text-gambit {
                font-size: 18vw;
            }
        }

        /* --- NAVIGATION --- */
        header {
            background: none;
            color: var(--black);
            padding: 0;
            margin: 0;
            box-shadow: none;
            z-index: 100;
            position: sticky;
            top: 0;
            border-bottom: none;
        }

        .navbar {
            display: flex;
            align-items: center;
            justify-content: space-between;
            max-width: 1200px;
            margin: 0 auto;
            padding: 1.2rem 2vw 1.2rem 2vw;
            background: rgba(255, 255, 255, 0.92);
            border-radius: 0 0 18px 18px;
            box-shadow: 0 2px 24px 0 rgba(0, 0, 0, 0.04);
            position: relative;
        }

        .navbar-logo {
            font-weight: 900;
            font-size: 1.5rem;
            letter-spacing: 0.01em;
            color: var(--green-dark);
            text-decoration: none;
            display: flex;
            align-items: center;
            gap: 0.6em;
            transition: color var(--transition), background var(--transition);
            padding: 0.2em 0.5em;
            border-radius: 8px;
        }

        .navbar-logo:hover,
        .navbar-logo:focus {
            color: var(--accent-dark);
            background: var(--beige);
        }

        .navbar-links {
            display: flex;
            align-items: center;
            gap: 2.2rem;
            list-style: none;
            margin: 0;
            padding: 0;
            /* --- NEW: Hide on mobile, show as overlay --- */
            transition: all 0.3s cubic-bezier(.4, 0, .2, 1);
        }

        .navbar-links a {
            color: var(--green-dark);
            text-decoration: none;
            font-weight: 700;
            font-size: 1.08rem;
            padding: 0.5em 1.2em;
            border-radius: 8px;
            transition: background var(--transition), color var(--transition), box-shadow var(--transition);
            position: relative;
            background: none;
            border: none;
            outline: none;
        }

        .navbar-links a:hover,
        .navbar-links a:focus {
            background: var(--accent);
            color: var(--black);
            box-shadow: 0 2px 12px 0 #e2c29033;
        }

        .navbar-toggle {
            display: none;
            background: none;
            border: none;
            font-size: 2.2rem;
            color: var(--green-dark);
            cursor: pointer;
            margin-left: 1em;
            padding: 0.2em 0.5em;
            border-radius: 8px;
            transition: background var(--transition), color var(--transition);
            z-index: 1201;
        }

        .navbar-toggle:focus,
        .navbar-toggle:hover {
            background: var(--beige);
            color: var(--accent-dark);
        }

        /* Hamburger icon bars */
        .navbar-toggle .bar {
            display: block;
            width: 30px;
            height: 3px;
            margin: 5px 0;
            background: var(--green-dark);
            border-radius: 5px;
            transition: transform 0.3s, opacity 0.3s;
        }

        .navbar-toggle.is-active .bar:nth-child(1) {
            transform: translateY(8px) rotate(45deg);
        }

        .navbar-toggle.is-active .bar:nth-child(2) {
            opacity: 0;
        }

        .navbar-toggle.is-active .bar:nth-child(3) {
            transform: translateY(-8px) rotate(-45deg);
        }

        /* --- NEW: Fullscreen mobile menu overlay --- */
        @media (max-width: 700px) {
            .navbar {
                flex-direction: row;
                padding: 1.2rem 0.7rem;
            }

            .navbar-links {
                display: none;
                flex-direction: column;
                position: fixed;
                top: 0;
                left: 0;
                width: 100vw;
                height: 100vh;
                background: rgba(255, 255, 255, 0.98);
                box-shadow: 0 8px 32px 0 rgba(0, 0, 0, 0.08);
                border-radius: 0;
                min-width: unset;
                z-index: 1200;
                padding: 0;
                gap: 0;
                justify-content: center;
                align-items: center;
                opacity: 0;
                pointer-events: none;
                transition: opacity 0.25s cubic-bezier(.4, 0, .2, 1);
            }

            .navbar-links.open {
                display: flex;
                opacity: 1;
                pointer-events: auto;
            }

            .navbar-links a {
                width: 80vw;
                max-width: 340px;
                text-align: center;
                padding: 1.2em 0;
                border-radius: 12px;
                border-bottom: none;
                font-size: 1.25rem;
                margin: 0.2em 0;
                background: none;
                box-shadow: 0 1px 8px 0 #e2c29011;
            }

            .navbar-links a:active {
                background: var(--accent);
                color: var(--black);
            }

            .navbar-toggle {
                display: block;
                position: relative;
                z-index: 1201;
            }

            /* --- Remove overlay close area from here --- */
            .navbar-links::before {
                content: none !important;
                display: none !important;
            }

            /* --- New: overlay background for mobile menu --- */
            .navbar-overlay {
                display: none;
                position: fixed;
                top: 0;
                left: 0;
                width: 100vw;
                height: 100vh;
                background: rgba(255, 255, 255, 0.98);
                z-index: 1199;
                transition: opacity 0.25s cubic-bezier(.4, 0, .2, 1);
                opacity: 0;
            }

            .navbar-overlay.open {
                display: block;
                opacity: 1;
                pointer-events: auto;
            }
        }

        /* --- NEW: Animate menu fade-in/out --- */
        .navbar-links,
        .navbar-links.open {
            will-change: opacity;
        }

        @media (max-width: 900px) {
            .navbar {
                padding: 1.2rem 0.5em;
            }
        }

