        /* --- HERO --- */
        .hero {
            min-height: 420px;
            text-align: center;
            position: relative;
            overflow: visible;
            z-index: 2;
            display: flex;
            align-items: center;
            justify-content: center;
            margin-top: 0.5em;
            border-bottom: 1.5px solid #e2c29044;
        }

        .hero-content {
            background: none;
            border-radius: 0;
            box-shadow: none;
            border: none;
            max-width: 1000px;
            margin: 0 auto;
            padding: 4.5rem 2vw 2.5rem 2vw;
            animation: fadein 1.2s cubic-bezier(.4, 0, .2, 1);
            transition: none;
            display: flex;
            flex-direction: column;
            align-items: center;
        }

        .hero-title {
            font-size: 3.2rem;
            font-weight: 900;
            letter-spacing: 0.01em;
            margin-bottom: 0.2em;
            color: var(--green-dark);
            text-shadow: 0 2px 12px #e2c29033;
            line-height: 1.05;
            background: linear-gradient(90deg, #e2c290 30%, #fffbe8 100%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }

        .hero-subtitle {
            color: var(--black);
            font-size: 1.35rem;
            font-weight: 500;
            margin-bottom: 1.2em;
            letter-spacing: 0.01em;
            text-shadow: 0 1px 8px #e2c29022;
            max-width: 600px;
        }

        .hero-description {
            color: var(--muted);
            font-size: 1.13rem;
            margin: 1.2em 0 0.5em 0;
            max-width: 600px;
            text-align: center;
        }

        .badge-row {
            margin: 1.5em 0 0.5em 0;
            display: flex;
            flex-wrap: wrap;
            gap: 0.7em;
            justify-content: center;
        }

        .badge-row img {
            background: var(--gray);
            border-radius: 7px;
            padding: 0.2em 0.7em;
            height: 28px;
            box-shadow: 0 1px 8px 0 #e2c29022;
            border: 1px solid #e2c29033;
        }

        .hero-cta-row {
            display: flex;
            justify-content: center;
            gap: 1.2em;
            margin: 2.2em 0 1.2em 0;
            flex-wrap: wrap;
        }

        .main-button {
            display: inline-block;
            padding: 1.1em 2.7em;
            background: linear-gradient(90deg, var(--green) 80%, var(--green-dark) 100%);
            color: var(--white);
            border-radius: 14px;
            font-weight: 900;
            font-size: 1.15rem;
            text-decoration: none;
            border: 2.5px solid var(--green);
            letter-spacing: 0.01em;
            transition: background var(--transition), color var(--transition), border var(--transition), box-shadow var(--transition), transform var(--transition);
            box-shadow: 0 2px 16px 0 rgba(59, 94, 43, 0.08);
            cursor: pointer;
            outline: none;
        }

        .main-button:hover,
        .main-button:focus {
            background: var(--white);
            color: var(--green-dark);
            border: 2.5px solid var(--green-dark);
            box-shadow: 0 4px 24px 0 rgba(59, 94, 43, 0.13);
            transform: translateY(-2px) scale(1.03);
        }

        .main-button.download {
            background: linear-gradient(90deg, var(--accent) 80%, var(--accent-dark) 100%);
            color: var(--black);
            border: 2.5px solid var(--accent-dark);
        }

        .main-button.download:hover,
        .main-button.download:focus {
            background: var(--green);
            color: var(--white);
            border: 2.5px solid var(--green-dark);
        }

        .alert {
            background: none;
            color: var(--green-dark);
            border-radius: 0;
            padding: 0.7em 0 0 0;
            font-weight: 700;
            margin-bottom: 1.2em;
            font-size: 1.02rem;
            text-align: center;
            box-shadow: none;
            border: none;
            display: block;
        }

        /* Remove Discord sticker styles */
        .discord-link {
            display: none !important;
        }

        /* --- HERO ENHANCEMENTS --- */
